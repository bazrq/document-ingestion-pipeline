{
    "version": "2.0",
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            },
            "enableLiveMetricsFilters": true
        }
    },
    "functionTimeout": "00:10:00",
    "extensions": {
        "queues": {
            "maxDequeueCount": 5
        },
        "http": {
            "routePrefix": "api",
            "cors": {
                "allowedOrigins": [
                    "http://localhost:5173",
                    "http://localhost:5174",
                    "http://127.0.0.1:5173"
                ],
                "supportCredentials": true
            }
        }
    }
}